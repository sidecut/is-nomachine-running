  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {
        "ISNO_PORT": "1234",
        "ISNO_SSLPORT": "2345"
      },
      "args": []
    },
    {
      "type": "lldb",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder:is-nomachine-running}",
      "name": "Debug App",
      "program": "${workspaceFolder:is-nomachine-running}/.build/debug/App",
      "preLaunchTask": "swift: Build Debug App"
    },
    {
      "type": "lldb",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder:is-nomachine-running}",
      "name": "Release App",
      "program": "${workspaceFolder:is-nomachine-running}/.build/release/App",
      "preLaunchTask": "swift: Build Release App"
    }
  ]
}
